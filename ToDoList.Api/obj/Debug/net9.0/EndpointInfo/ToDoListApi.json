        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ToDo/Update/{id}": {
      "put": {
        "tags": [
          "ToDo"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ToDoItem"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ToDoItem"
