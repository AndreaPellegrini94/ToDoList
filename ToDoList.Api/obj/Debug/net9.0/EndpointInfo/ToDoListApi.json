            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ToDoItem"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ToDo/Delete/{id}": {
      "delete": {
        "tags": [
          "ToDo"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
